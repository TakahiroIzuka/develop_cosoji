@charset "utf-8";/*------------------------------header------------------------------*/.header{    padding: 26px 0;}.header-inner{    display: flex;    align-content: center;    justify-content: space-between;    padding: 0 110px;}.header-link{    text-align: center;    display: flex;    align-items: center;}.btn_sm{    font-size: 1.4rem;    font-weight: 600;    line-height: 1;    text-decoration: none;    display: block;        border-radius: 19px;    padding: 11px 8px 10px 25px ;}.header-link-btn_login{    margin-right: 16px;}.header-link-btn_login .btn_sm{    color: #333;    border: 1px solid #333;    position: relative;    width: 134px;    }.header-link-btn_regster .btn_sm{    color: #fff;    background-color: #1dbff2;    position: relative;    width: 151px;}.header-link-btn_login .btn_sm::after{	content: '';	display: inline-block;	width: 17px;	height: 17px;	background-image: url("../images/worker/login.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;		position: absolute;	top: 50%;	left: 14px;    transform: translate(0,-50%);}.header-link-btn_regster .btn_sm::after{	content: '';	display: inline-block;	width: 16px;	height: 19px;	background-image: url("../images/worker/register.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;		position: absolute;	top: 50%;	left: 17px;    transform: translate(0,-50%);}/*------------------------------mv------------------------------*/.mv{    text-align: center;    }.mv-inner{    max-width: 100%;    height: calc(100vh - 89px);    overflow: hidden;    position: relative;}.mv-bg{    position: absolute;    top: 0;    left: 0;    z-index: 2;    display: block;    width: 100%;    height: 100%;    background-image: url("../images/bg_stripe.png");    background-repeat: repeat;    background-size: 11px 11px;    background-position: center;    background-color: rgba(0,0,0,0.2);}.mv-video{    object-fit: cover;    height: 100%;    width: 100%;    position: relative;    z-index: 1;    top: 0;    left: 0;    transform: none;}.mv-container{    position: absolute;    z-index: 10;    top: 50%;    left: 50%;    transform: translate(-50%,-50%);}.mv-container-logo{    margin-bottom: 20px;}.mv-container-catch{    margin-bottom: 26px;}.mv-container-text{    font-size: 20px;    color: #fff;    font-weight: 600;    line-height: 1;    margin-bottom: 64px;}.mv-container-login>a{    color: #fff;    font-size: 1.5rem;    font-weight: 600;    text-decoration: none;    letter-spacing: 0.05em;}/*------------------------------catch------------------------------*/.catch{    padding-top: 48px;    padding-bottom: 51px;}.catch-row{    display: flex;    align-items: center;}.catch-row-img{    width: 50%;}.catch-row-text{    width: 50%;    padding-left: calc(61 / 1220 * 100%);}.catch-row-text-title{    font-size: 2.6rem;    font-weight: 600;    line-height: 1.38;    margin-bottom: 27px;}/*------------------------------feature------------------------------*/.feature{    overflow: hidden;    background-color: #bbecfb;    padding-top: 55px;    padding-bottom: 69px;}.feature-title{    font-size: 3.4rem;    font-weight: 600;    text-align: center;    margin-bottom: 56px;}.feature-list{    display: flex;    flex-wrap: wrap;    justify-content: space-between;    margin-right: calc(-53 / 1220 * 100% );}.feature-item{    width: 33.3333%;    padding-right: calc(53 / 1220 * 100% );}.feature-item-inner{    display: flex;}.feature-item-img{    width: calc(151 / 362 * 100%);}.feature-item-text{    width: calc(100% - 151px);    padding-left: 25px;    padding-top: 25px;}.feature-item-text-number{    position: relative;    margin-bottom: 21px;    padding-left: 33px;}.feature-item-text-number::before{    content: "";    display: inline-block;    width: 20px;    height: 3px;    background-color: #333;    position: absolute;    top: 50%;    left: 0;    transform: translate(0,-50%);    margin-right: 10px;    }.feature-item-text-title{    font-size: 2rem;    font-weight: 600;    line-height: 1.25;    margin-bottom: 15px;}.feature-item-text-paragraph{    font-size: 1.4rem;    line-height: 1.5;}/*------------------------------contents common------------------------------*/.contents{    text-align: center;    padding-top: 93px;}.contents-title{    font-size: 3.6rem;    font-weight: 600;    line-height: 1.25;}.worker .contents-title{    display: inline-block;    border-bottom: 4px solid #f5b5b5;    position: relative;    padding: 0 24px 23px;    margin-bottom: 85px;}.worker .contents-title:before{    content: '';    position: absolute;    display: block;    width: 0;    height: 0;    bottom: -30px;    left: 50%;    transform: translate(-50%,0);    border-right: 27px solid transparent;    border-top: 30px solid #f5b5b5;    border-left: 27px solid transparent;}.worker .contents-title:after{    content: '';    position: absolute;    display: block;    width: 0;    height: 0;    bottom: -24px;    left: 50%;    transform: translate(-50%,0);    border-right: 27px solid transparent;    border-top: 30px solid #fff;    border-left: 27px solid transparent;}/*------------------------------recommend------------------------------*/.recommend{    background-color: #f4f4f4;}.recommend .contents-title:after{    border-top: 30px solid #f4f4f4;}.recommend-list{    }.recommend-item{    }.recommend-item-inner{    display: flex;    align-items: center;    position: relative;}.recommend-item:nth-of-type(even) .recommend-item-inner{    flex-direction: row-reverse;}.recommend-item-img{    width: 50%;}.recommend-item-img>img{    width: 100%;}.recommend-item-text{    text-align: left;    width: 50%;    max-width: 675px;    padding-left: 57px;}.recommend-item-title{    font-size: 2.6rem;    font-weight: 600;    line-height: 1.25;    position: relative;    margin-bottom: 28px;    padding-left: 25px;}.recommend-item-title:before{    content: "";    display: inline-block;    width: 16px;    height: 3px;    background-color: #bbecfb;    position: absolute;    top: 50%;    left: 0;    transform: translate(0,-50%);}.recommend-item-paragraph{    }.recommend-item-paragraph-note{    font-size: 1.4rem;    color: #777;    margin-top: 24px;}@media all and (min-width: 1800px){    .recommend-item-img{        text-align: right;    }    .recommend-item-img>img{        width: 1000px;    }    .recommend-item:nth-of-type(even) .recommend-item-img{        text-align: left;    }}/*------------------------------usage------------------------------*/.usage{    overflow: hidden;}.usage-list{    display: flex;    flex-wrap: wrap;    justify-content: space-between;    margin-right: -53px;}.usage-item{    width: 25%;    position: relative;    padding-right: 53px;}.usage-item:not(:last-of-type)::after{	content: '';	display: inline-block;	width: 20px;	height: 24px;	background-image: url("../images/usage_arrow.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;	position: absolute;	top: 50px;	right: 5%;}.usage-item-inner{    border-top: 3px solid #bbecfb;    padding: 27px 15px 0;}.usage-item-number{    margin-bottom: 44px;}.usage-item-title{    font-size: 2.4rem;    font-weight: 600;    line-height: 1.25;    margin-bottom: 26px;}.usage-item-paragraph{    text-align: left;    line-height: 1.5;    margin-bottom: 24px;}.usage-item-img{    }/*------------------------------link area------------------------------*/.linkarea{    text-align: center;    margin-top: 100px;    padding-top: 144px;    padding-bottom: 142px;}.linkarea_01{    background-image: url("../images/linkarea_01.jpg");    background-repeat: no-repeat;    background-size: cover;    background-position: center right;    background-attachment: fixed;}.linkarea_02{    background-image: url("../images/linkarea_02.jpg");    background-repeat: no-repeat;    background-size: cover;    background-position: center;    background-attachment: fixed;}.linkarea-container{    width: 50%;}.linkarea-container-text{    color: #fff;    font-size: 3.4rem;    font-weight: 600;    margin-bottom: 67px;}.linkarea-container-link>a{    }.btn{    color: #fff;    font-weight: 600;    line-height: 1;    text-decoration: none;    display: block;    width: 269px;    border-radius: 31px;    padding: 21px 15px 24px;    margin: 0 auto;}.worker .btn{    background-color: #1dbff2;}/*------------------------------kinds------------------------------*/.kinds{    overflow: hidden;}.kinds-list{    display: flex;    flex-wrap: wrap;    justify-content: space-between;    margin-right: -56px;}.kinds-item{    width: 50%;    padding-right: 56px;    margin-bottom: 39px;}.kinds-item:nth-of-type(n+3){    margin-bottom: 0;}.kinds-item-inner{    }.kinds-item-img{    margin-bottom: 13px;}.kinds-item-text{    text-align: left;    display: flex;    align-items: center;    justify-content: flex-start;    padding: 0 23px;}.kinds-item-title{    color: #1dbff2;    font-size: 2.4rem;    font-weight: 600;    line-height: 1;    width: 6em;    position: relative;    padding-left: 29px;}.kinds-item-title::before{    content: "";    display: inline-block;    width: 16px;    height: 3px;    background-color: #bbecfb;    position: absolute;    top: 50%;    left: 0;    transform: translate(0,-50%);}.kinds-item-paragraph{    line-height: 1.5;    width: calc(100% - 6em);    padding-left: 52px;}/*------------------------------map------------------------------*/.map{    text-align: center;    background-color: #fef6f6;    position: relative;    padding-bottom: calc(194 / 1440 * 100vw);    padding-top: calc(180 / 1440 * 100vw);    margin-top: 178px;}.map-container{    width: 50%;}.map-container-text{    font-size: 3rem;    font-weight: 600;    line-height: 1.5;    margin-bottom: 44px;}.map-img{    position: absolute;    top: -56px;    right: 0;    z-index: 10;}.map-img>img{    width: calc(810 / 1440 * 100vw);    }@media all and (min-width: 1500px){    .map{      padding-bottom: 202px;    padding-top: 187px;}    .map-img>img{        width: 843px;        }}/*------------------------------voice------------------------------*/.voice-list{    }.voice-item{    text-align: left;    display: flex;    align-items: center;    justify-content: space-between;    position: relative;    margin-bottom: 46px;}.voice-item:nth-of-type(even){    flex-direction: column-reverse;}.voice-item-inner{    max-width: 1090px;    width: 100%;    border: 7px solid #ececec;    border-radius: 20px;    padding-top: 70px;    padding-bottom: 58px;    padding-left: 60px;}.voice-item:nth-of-type(even) .voice-item-inner{    margin-left: auto;    padding-left: 317px;}.voice-item-text{    }.voice-item-title{    font-size: 2.6rem;    font-weight: 600;    line-height: 1.25;    display: inline-block;    position: relative;    margin-bottom: 25px;}.voice-item-title::before{	content: '';	display: inline-block;	width: 20px;	height: 17px;	background-image: url("../images/voice_dot_before.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;	position: absolute;	top: -10px;	left: -27px;}.voice-item-title::after{	content: '';	display: inline-block;	width: 20px;	height: 17px;	background-image: url("../images/voice_dot_after.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;	position: absolute;	top: -10px;	right: -20px;}.voice-item-name{    font-size: 2rem;    font-weight: 600;    line-height: 1.25;    margin-bottom: 27px;}.voice-item-paragraph{    max-width: 647px;}.voice-item-img{    position: absolute;    right: 0;    top: 50%;    transform: translate(0,-50%);}.voice-item:nth-of-type(even) .voice-item-img{    right: auto;    left: 0;}/*------------------------------qanda------------------------------*/.qanda-list-wrapper{    border-top: 1px solid #b3b3b3; }.qanda-list{    text-align: left;    }.qanda-title{    position: relative;    cursor: pointer;}.qanda-title::after{	content: '';	display: inline-block;	width: 22px;	height: 12px;	background-image: url("../images/qanda_arrow.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;	position: absolute;	top: 50%;	right: 38px;    transform: translate(0,-50%);    transition: all 0.2s;}.qanda-title.active::after{    transform: translate(0,-50%) rotate( 180deg);/*    transform-origin: center;*/}.qanda-title-inner{    font-size: 1.8rem;    line-height: 1.5;    font-weight: 500;    border-bottom: 1px solid #b3b3b3;    padding: 26px 120px 29px 137px;    padding-left: 137px;}.qanda-title-text{    position: relative;        padding-left: 48px;}.qanda-title-text::before{	content: '';	display: inline-block;	width: 18px;	height: 24px;	background-image: url("../images/qanda_q.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;	position: absolute;	top: 50%;	left: 0;    transform: translate(0,-50%);}.qanda-data{    display: none;}.qanda-data-inner{    line-height: 1.5;    background-color: #ededed;    border-bottom: 1px solid #b3b3b3;     padding: 23px 120px 26px 137px;}.qanda-data-text{    position: relative;    padding-left: 48px;}.qanda-data-text::before{	content: '';	display: inline-block;	width: 19px;	height: 24px;	background-image: url("../images/qanda_a.png");	background-size: contain;	background-repeat: no-repeat;	vertical-align: middle;	position: absolute;	top: 50%;	left: 0;    transform: translate(0,-50%);}/*------------------------------footer------------------------------*/.footer{    background-color: #bbecfb;    padding-top: 44px;    margin-top: 98px;    padding-bottom: 26px;}.footer-nav{    text-align: right;}.footer-nav-list{    }.footer-nav-item{    margin-bottom: 22px;}.footer-nav-item>a{    color: #333;    font-weight: 500;    line-height: 1;    text-decoration: none;}.footer-text{    font-size: 1.8rem;    font-weight: 600;    margin-top: 74px;    margin-bottom: 102px;}.small-wrapper{    font-size: 1.4rem;    display: flex;}.small{    padding-right: 30px;}.footer-small-nav{    }.footer-small-nav-list{    display: flex;}.footer-small-nav-item{    padding-right: 33px;}.footer-small-nav-item>a{    color: #333;    text-decoration: none;}